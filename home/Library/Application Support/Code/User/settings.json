  "test-switcher.rules": [
    {
      "pattern": "(.*)\\.js",
      "replacement": "$1.spec.js"
    },
    {
      "pattern": "(.*)\\.spec\\.js",
      "replacement": "$1.js"
    },
    {
      "pattern": "(.*)\\.py",
      "replacement": "test/test_$1.py"
    },
    {
      "pattern": "test/test_(.*)\\.py",
      "replacement": "$1.py"
    },
    {
      "pattern": "models/(.*)\\.py",
      "replacement": "test/test_models_$1.py"
    },
    {
      "pattern": "test/test_models_(.*)\\.py",
      "replacement": "models/$1.py"
    },
    {
      "pattern": "helpers/(.*)\\.py",
      "replacement": "test/test_helpers_$1.py"
    },
    {
      "pattern": "test/test_helpers_(.*)\\.py",
      "replacement": "helpers/$1.py"
    },
    {
      "pattern": "utils/(.*)\\.py",
      "replacement": "test/test_utils_$1.py"
    },
    {
      "pattern": "test/test_utils_(.*)\\.py",
      "replacement": "utils/$1.py"
    }
  ],
  "tidyHtml.formatOnSave": false,
